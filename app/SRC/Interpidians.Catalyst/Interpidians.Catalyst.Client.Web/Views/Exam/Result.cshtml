@model Interpidians.Catalyst.Core.Entity.ExamResult
@{
    Layout = null;
    ViewBag.Title = "Result";
}

<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
 <!--<![endif]-->
<head>
    <title>Catalyst | Login | Register</title>

    <!-- Meta -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Favicon -->
    <link rel="shortcut icon" href="favicon.ico">

    <!-- Web Fonts -->
    <link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600&amp;subset=cyrillic,latin'>

    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="~/Content/plugins/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/css/app.css">
    <link rel="stylesheet" href="~/Content/css/blocks.css">

    <link rel="stylesheet" href="~/Content/css/sky-forms.css">
    <link rel="stylesheet" href="~/Content/css/shop.style.css">
    <link rel="stylesheet" href="~/Content/css/catalyst.css">

    <!-- CSS Header and Footer -->
    <link rel="stylesheet" href="~/Content/css/headers/header-v5.css">
    <link rel="stylesheet" href="~/Content/css/footers/footer-v4.css">

    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="~/Content/plugins/animate.css">
    <link rel="stylesheet" href="~/Content/plugins/line-icons/line-icons.css">
    <link rel="stylesheet" href="~/Content/plugins/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/plugins/scrollbar/css/jquery.mCustomScrollbar.css">

    <!-- Style Switcher -->
    <!--link rel="stylesheet" href="~/Content/css/plugins/style-switcher.css">

    <!-- CSS Theme -->
    <link rel="stylesheet" href="~/Content/css/theme-colors/green.css" id="style_color">

    <!-- CSS Customization -->
    <link rel="stylesheet" href="~/Content/css/catalyst.css">

    <!-- CSS Page Style -->
    <link rel="stylesheet" href="~/Content/css/pages/mcq-test.css">

    <!-- Global Js Variables -->
    <script type="text/javascript">
        var page = "mcq-result";
    </script>
</head>

<body class="header-fixed">

    <div class="wrapper">
        <!--=== Breadcrumbs v4 ===-->
        <div class="breadcrumbs">

            <div class="container">
                <h1 class="pull-left">MCQ Result</h1>

            </div>
        </div>
        <!--=== End Breadcrumbs v4 ===-->
        <!--=== MCQs ===-->
        <div class="content-md ">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="mcqresult-body">

                            <div class="row">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <!-- Pie Charts v1 -->
                                    <div class="pie-progress-charts margin-top-20 margin-bottom-60">
                                        <div class="inner-pchart">
                                            <div class="circle" id="resultCircle"></div>
                                            @*<div class="circle" id="circle-1"></div>*@
                                            <h3 class="circle-title">Test Name</h3>
                                            <p>Demo Test</p>
                                        </div>
                                    </div>
                                    <!-- End Pie Charts v1 -->
                                </div>

                                <div class="col-sm-4">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <!-- Progress Bar Text -->
                                            <div class="progress-bar-text">
                                                <p class="text-left pull-left">Correct</p>
                                                <p class="text-right">@(Model.NoOfCorrectAnswer)</p>
                                                @*<div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-u" role="progressbar" aria-valuenow="@(Model.NoOfCorrectAnswer/)" aria-valuemin="0" aria-valuemax="100" style="width: @(Model.NoOfCorrectAnswer)%">
                                                    </div>
                                                </div>*@
                                            </div>
                                            <!-- End Progress Bar Text -->
                                            <!-- Progress Bar Text -->
                                            <div class="progress-bar-text">
                                                <p class="text-left pull-left">Incorrect</p>
                                                <p class="text-right">@(Model.NoOfIncorrectAnswer)</p>
                                                @*<div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-red" role="progressbar" aria-valuenow="@(Model.NoOfIncorrectAnswer)" aria-valuemin="0" aria-valuemax="100" style="width: @(Model.NoOfIncorrectAnswer)%">
                                                    </div>
                                                </div>*@
                                            </div>
                                            <!-- End Progress Bar Text -->
                                            <!-- Progress Bar Text -->
                                            <div class="progress-bar-text">
                                                <p class="text-left pull-left">Skipped & Review</p>
                                                <p class="text-right">@(Model.NoOfSkippedAnswer + Model.NoOfMarkForReviewAnswer)</p>
                                                @*<div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-dark progress-bar-u-danger" role="progressbar" aria-valuenow="@(Model.NoOfSkippedAnswer + Model.NoOfMarkForReviewAnswer)" aria-valuemin="0" aria-valuemax="100" style="width: @(Model.NoOfSkippedAnswer + Model.NoOfMarkForReviewAnswer)%">
                                                    </div>
                                                </div>*@
                                            </div>
                                            <!-- End Progress Bar Text -->
                                            <!-- Progress Bar Text -->
                                            <div class="progress-bar-text">
                                                <p class="text-left pull-left">Time Spent</p>
                                                <p class="text-right">@(Model.TimeSpent.Value)</p>
                                                <div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-purple progress-bar-u-danger" role="progressbar" aria-valuenow="@(Model.TimeSpentPercentage)" aria-valuemin="0" aria-valuemax="100" style="width: @(Model.TimeSpentPercentage)%">
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- End Progress Bar Text -->
                                            <!-- Progress Bar Text -->
                                            @*<div class="progress-bar-text">
                                                <p class="text-left pull-left">Avarage Accuracy</p>
                                                <p class="text-right">10%</p>
                                                <div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-orange progress-bar-u-danger" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width: 10%">
                                                    </div>
                                                </div>
                                            </div>*@
                                            <!-- End Progress Bar Text -->
                                            <!-- Progress Bar Text -->
                                            <div class="progress-bar-text">
                                                <p class="text-left pull-left">Result</p>
                                                <p class="text-right">@(Model.Percentage)%</p>
                                                <div class="progress progress-u progress-xs">
                                                    <div class="progress-bar progress-bar-blue" role="progressbar" aria-valuenow="@(Model.Percentage)" aria-valuemin="0" aria-valuemax="100" style="width: @(Model.Percentage)%">
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- End Progress Bar Text -->
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Profile Content -->
                </div><!--/end row-->
            </div><!--/end container-->
        </div>
        <!--=== End MCQs ===-->
        <!--=== Some Text ===-->
        <div class="shop-subscribe">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 md-margin-bottom-20">
                        <h2>Test Complete</h2>
                    </div>
                </div>
            </div><!--/end container-->
        </div>
        <!--=== End Some Text ===-->
        <!--=== Footer v4 ===-->
        <div class="footer-v4">
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <p>
                                2016 &copy; ALL Rights Reserved.
                                <a href="#">Privacy Policy</a> | <a href="#">Terms of Service</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div><!--/copyright-->
        </div>
        <!--=== End Footer v4 ===-->
    </div><!--/wrapper-->
    <!-- JS Global Compulsory -->
    <script src="~/Content/plugins/jquery/jquery.min.js"></script>
    <script src="~/Content/plugins/jquery/jquery.plugin.js"></script>
    <script src="~/Content/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- JS Implementing Plugins -->
    <script src="~/Content/plugins/back-to-top.js"></script>
    <script src="~/Content/plugins/smoothScroll.js"></script>
    <script src="~/Content/plugins/scrollbar/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Content/plugins/circles-master/circles.js"></script>

    <script src="~/Content/js/plugins/circles-master.js"></script>

    <!-- JS Common Level -->
    <script src="~/Content/js/common.js"></script>
    <!-- JS Customization -->
    <script src="~/Content/js/custom.js"></script>
    <script>
		jQuery(document).ready(function() {
			App.init();
			App.initScrollBar();
		    //CirclesMaster.initCirclesMaster1(); 
		    //CirclesMaster.initCirclesMaster2();
			CirclesMaster.initCirclesMasterCustom('resultCircle',''+@Model.Percentage);
		});
    </script>
